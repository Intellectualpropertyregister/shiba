<%= simple_form_for(@scheduled_job, html: { class: 'form-horizontal' }) do |f| %>  
  <%= f.association :city, collection: City.order(:name) %>
  <%= f.input :description %>
  <%= f.input :job_command, as: :select, collection: Dir.glob("app/workers/*") %>
  <%= f.input :cron_schedule %>
  <%= f.input :worker_name %>

  <%= f.submit "Create" %>
<% end %>